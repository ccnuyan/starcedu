{
  "name": "starcedu",
  "version": "1.0.0",
  "description": "starcedu",
  "main": "index.js",
  "author": "ccnuyan",
  "license": "ISC",
  "scripts": {
    "eslint": "node_modules/.bin/eslint --ignore-path .eslintignore ./**/*.js",
    "wp-dev": "cross-env NODE_ENV=development && webpack-dev-server --host 127.0.0.1",
    "wp-build": "cross-env NODE_ENV=production && webpack --config=webpack.build-config.js",
    "# comment - test #": "test scripts:",
    "test": "mocha --opts ./mocha.opts ./_auth/**/*.spec.js ./_disk/**/*.spec.js",
    "test-ci": "mocha --opts ./mocha.ci.opts ./_auth/**/*.spec.js ./_disk/**/*.spec.js",
    "start-tenant": "babel-node ./server/tenants/testTenant.js",
    "precommit": "lint-staged",
    "# comment - dev #": "dev scripts:",
    "start-auth-dev": "babel-node ./_auth",
    "start-disk-dev": "babel-node ./_disk",
    "server-dev": "nodemon --config nodemon.json --delay 1000ms",
    "start-dev": "npm-run-all -p wp-dev server-dev",
    "# comment - start #": "start scripts:",
    "start": "node server"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "# comment # - browserslist": "for postcss autoprefix, which is in ./webpack/postcss.config.js",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "connect-redis": "^3.3.3",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.1",
    "cross-fetch": "^1.1.1",
    "express": "^4.15.2",
    "express-delay": "^0.2.0",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.9.0",
    "pg": "^6.2.2",
    "qiniu": "^7.1.1",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "env-test": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "file-size": "^1.0.0",
    "fine-uploader": "^5.15.5",
    "html-loader": "^0.4.5",
    "husky": "^0.14.3",
    "immutable": "^3.8.1",
    "lint-staged": "^3.6.1",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-loader": "^1.3.3",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nested": "^1.0.1",
    "postcss-nesting": "^2.3.1",
    "postcss-partial-import": "^3.1.1",
    "postcss-selector-matches": "^2.0.5",
    "postcss-selector-not": "^2.0.0",
    "postcss-url": "^6.0.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0"
  },
  "#comment# - eslintConfig": "http://eslint.org/docs/user-guide/configuring",
  "#comment# - eslint-plugin-react": "https://github.com/yannickcr/eslint-plugin-react",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "babel-eslint",
    "plugins": [
      "standard",
      "import",
      "mocha",
      "jsx-a11y",
      "react",
      "promise"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:mocha/recommended",
      "plugin:promise/recommended",
      "airbnb/base"
    ],
    "env": {
      "mocha": true,
      "browser": true,
      "es6": true
    },
    "globals": {
      "$": true,
      "chai": true,
      "expect": true,
      "should": true
    },
    "rules": {
      "class-methods-use-this": 0,
      "promise/always-return": 0,
      "camelcase": 0,
      "no-underscore-dangle": 0,
      "consistent-return": 0,
      "no-unused-expressions": 0,
      "max-len": 0,
      "import/no-extraneous-dependencies": 0,
      "arrow-body-style": 0,
      "no-return-assign": 0,
      "jsx-a11y/href-no-hash": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-closing-bracket-location": 2,
      "react/no-set-state": 0,
      "id-length": 0,
      "react/jsx-curly-spacing": [
        2,
        "always"
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "object-curly-spacing": [
        2,
        "always"
      ]
    }
  }
}